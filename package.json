{
  "name": "nodejs-product-api",
  "version": "1.0.0",
  "description": "CRUD sample using inversify-express and mongodb",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --timeout 0 -r ts-node/register tests/**/*.test.ts --exit",
    "start": "NODE_ENV=local nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "update": "updates --update --minor && npm install",
    "postupdate": "git diff-files --quiet package-lock.json || npm test"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "helmet": "3.20.0",
    "inversify": "5.0.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "inversify-logger-middleware": "3.1.0",
    "mongodb": "3.3.1",
    "morgan": "1.9.1",
    "pg": "7.12.1",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.18",
    "mongoose": "~5.5.6"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.27",
    "@types/body-parser": "1.17.1",
    "@types/chai": "4.2.0",
    "@types/express": "4.17.1",
    "@types/helmet": "0.0.44",
    "@types/mocha": "5.2.7",
    "@types/mongodb": "3.3.0",
    "@types/morgan": "1.7.37",
    "@types/mongoose": "~5.5.0",
    "mongodb-memory-server-core": "~5.1.0",
    "@types/lodash": "4.14.123",
    "chai": "4.2.0",
    "chai-http": "^4.2.0",
    "mocha": "6.2.0",
    "nyc": "14.1.1",
    "ts-node": "8.3.0",
    "typescript": "3.5.3",
    "updates": "^8.5.3",
    "node-config-ts": "^2.1.2"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.ts"
    ]
  },
  "author": "",
  "license": "ISC"
}
